{
  "rewrites": [
    {
      "source": "/login",
      "destination": "/api/hand?path=/login"
    },
    {
      "source": "/dashboard",
      "destination": "/api/hand?path=/dashboard"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ]
}
